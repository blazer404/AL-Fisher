{
  "manifest_version": 3,
  "name": "Libria Interceptor",
  "version": "0.4.1",
  "description": "Анус себе заблокируй",
  "developer": {
    "name": "BlazeR404",
    "url": "https://github.com/blazer404"
  },
  "icons": {
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "permissions": [],
  "content_scripts": [
    {
      "js": [
        "js/main.js"
      ],
      "matches": [
        "*://anilibria.top/*",
        "*://anilibria.wtf/*"
      ],
      "run_at": "document_start",
      "all_frames": true,
      "world": "MAIN"
    }
  ]
}
